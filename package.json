{
  "name": "@getinapp/appetizer",
  "version": "1.2.2",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Get In",
  "description": "Appetizer é uma UI Kit em React que ajuda os desenvolvedores e designers do @getinapp a construir interfaces de usuário consistentes e sem esforço.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getinapp/appetizer.git"
  },
  "files": [
    "dist/**",
    "public/**"
  ],
  "keywords": [
    "design",
    "react",
    "ui",
    "components",
    "library"
  ],
  "bugs": {
    "url": "https://github.com/getinapp/appetizer/issues"
  },
  "homepage": "https://github.com/getinapp/appetizer#readme",
  "scripts": {
    "build": "tsc -d",
    "release": "auto shipit --no-changelog --base-branch=master --email=wendelfreitasbranco@gmail.com",
    "commit": "git-cz",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "test:watch": "yarn test --watchAll",
    "pre-commit-lint": "yarn typecheck && yarn lint-staged",
    "make:component": "hygen new component"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "auto": "^10.32.5",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.3.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/index.tsx",
      "src/assets/*",
      "src/store/index.ts",
      ".*__snapshots__/.*",
      "dist/*"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts(x)?",
      "!src/**/*.stories.tsx",
      "!src/index.ts",
      "!src/styles/**"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/.jest/jest-setup.ts"
    ],
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@styled-icons/fluentui-system-filled": "^10.35.0",
    "@styled-icons/ionicons-solid": "^10.34.0",
    "react-modal": "^3.14.4",
    "styled-components": "^5.3.3"
  }
}
